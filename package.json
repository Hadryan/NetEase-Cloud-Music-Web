{
  "name": "netease-cloud-music-web",
  "version": "1.1.3",
  "description": "",
  "scripts": {
    "dev": "npm run build-dll & webpack-dev-server",
    "build": "npm run eslint && npm run build-dll && webpack --config webpack.config.production.babel.js",
    "build-dll": "npm run clean && webpack --config webpack.config.dll.babel.js",
    "test": "set NODE_ENV=test  && node ./node_modules/karma/bin/karma start ./spec/karma.conf.js",
    "test-ci": "node ./node_modules/karma/bin/karma start ./spec/karma.conf.ci.js",
    "coveralls": "cat ./spec/coverage/lcov/lcov.info | ./node_modules/.bin/coveralls",
    "eslint": "node ./node_modules/eslint/bin/eslint.js --ext .js,jsx ./",
    "stylelint": "node ./node_modules/stylelint/bin/stylelint.js src/**/*.css",
    "clean": "rimraf dist/*",
    "gh-pages": "npm run build && gh-pages -d dist",
    "analyze": "webpack --profile --json > assets/bundle-analyzer-stats.json",
    "analyze-windows": "webpack --profile --json | Out-file 'assets/bundle-analyzer-stats.json' -Encoding OEM"
  },
  "author": "shianqi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shianqi/NetEase-Cloud-Music-Web.git"
  },
